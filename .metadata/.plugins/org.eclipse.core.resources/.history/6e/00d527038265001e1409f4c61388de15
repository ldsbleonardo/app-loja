package br.edu.infnet.applojaroupas;

import org.springframework.boot.ApplicationArguments;
import org.springframework.boot.ApplicationRunner;
import org.springframework.core.annotation.Order;
import org.springframework.stereotype.Component;

import br.edu.infnet.applojaroupas.model.negocio.Vendas;

@Order(3)
@Component
public class AcessorioLoader implements ApplicationRunner {

	@Override
	public void run(ApplicationArguments args) throws Exception {
		Vendas v1 = new Vendas(1, 1, "Roupas Plus Size", "LoluModas", "G", 2.5, 10, 5);
		System.out.println("Inclusao de venda: " + v1);

	}

}
