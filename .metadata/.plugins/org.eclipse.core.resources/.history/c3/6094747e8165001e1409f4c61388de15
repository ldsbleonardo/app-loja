package br.edu.infnet.applojaroupas.model.negocio;

public class Acessorio extends Produto {

	String tipo;
	String cor;
	String tamanho;

	public Acessorio(Integer codigoProduto, Integer codigoFornecedor, String descricao, String marca, String tamanho,
			Double valorAquisicao) {
		super(codigoProduto, codigoFornecedor, descricao, marca, tamanho, valorAquisicao);
	}

}
